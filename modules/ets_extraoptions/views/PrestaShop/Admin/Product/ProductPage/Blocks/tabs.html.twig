<div class="tabs js-tabs">
  <div class="arrow d-xl-none js-arrow left-arrow float-left">
    <i class="material-icons hide">chevron_left</i>
  </div>

  <ul class="nav nav-tabs js-nav-tabs" id="form-nav" role="tablist">
    <li id="tab_step1" class="nav-item"><a href="#step1" role="tab" data-toggle="tab" class="nav-link active">{{ 'Basic settings'|trans({}, 'Admin.Catalog.Feature') }}</a></li>
    <li id="tab_step3" class="nav-item"><a href="#step3" role="tab" data-toggle="tab" class="nav-link">{{ 'Quantities'|trans({}, 'Admin.Catalog.Feature') }}</a></li>
    <li id="tab_step4" class="nav-item"><a href="#step4" role="tab" data-toggle="tab" class="nav-link">{{ 'Shipping'|trans({}, 'Admin.Catalog.Feature') }}</a></li>
    <li id="tab_step2" class="nav-item"><a href="#step2" role="tab" data-toggle="tab" class="nav-link">{{ 'Pricing'|trans({}, 'Admin.Catalog.Feature') }}</a></li>
    {% if etsCzfFormProduct is defined and etsCzfFormProduct %}
      <li id="tab_ets_czf" class="nav-item"><a href="#ets_czf" role="tab" data-toggle="tab" class="nav-link">{{ etsCzfTrans.custom_fields }}</a></li>
    {% endif %}
    {% if etsEptExtraTabs is defined and etsEptExtraTabs %}
      <li id="tab_ets_ept" class="nav-item"><a href="#ets_ept" role="tab" data-toggle="tab" class="nav-link">{{ etsEptTrans.Extra_tabs }}</a></li>
    {% endif %}
    <li id="tab_step5" class="nav-item"><a href="#step5" role="tab" data-toggle="tab" class="nav-link">
        {% if ets_seo_product_seo_analysis is defined %}
          {{ 'SEO settings'|trans({}, 'Modules.Etsseo.Admin') }}
        {% else %}
          {{ 'SEO'|trans({}, 'Admin.Catalog.Feature') }}
        {% endif %}
      </a></li>
    {% if ets_wholesale_product is defined %}
        <li id="tab_ets_wholesale" class="nav-item">
            <a href="#ets_wholesale" role="tab" data-toggle="tab" class="nav-link">{{ 'Wholesale'|trans({}, 'Modules.Etswholesale.Admin') }}</a>
        </li>
    {% endif %}
    {% if ets_seo_product_seo_analysis is defined %}
    <li id="tab_ets_seo_analysis" class="nav-item"><a href="#step_ets_seo_analysis" role="tab" data-toggle="tab" class="nav-link js-ets-seo-tab-customize">{{ 'SEO analysis'|trans({}, 'Modules.Etsseo.Admin') }}</a></li>
    {% endif %}
    <li id="tab_step_ets_seo_analysis" class="nav-item"><a href="#step6" role="tab" data-toggle="tab" class="nav-link">{{ 'Options'|trans({}, 'Admin.Global') }}</a></li>
    {% if hooksarraycontent(hooks) is not empty %}
      <li id="tab_hooks" class="nav-item"><a href="#hooks" role="tab" data-toggle="tab" class="nav-link">{{ 'Modules'|trans({}, 'Admin.Catalog.Feature') }}</a></li>
    {% endif %}
    {% if ets_extraoptions_product is defined %}
        <li id="tab_ets_extraoptions" class="nav-item">
            <a href="#ets_extraoptions" role="tab" data-toggle="tab" class="nav-link">{{ets_extraoptions_product_text}}</a>
        </li>
    {% endif %}
  </ul>
  <div class="arrow d-xl-none js-arrow right-arrow visible float-right">
    <i class="material-icons hide">chevron_right</i>
  </div>
</div>